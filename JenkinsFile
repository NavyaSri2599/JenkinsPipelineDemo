pipeline {
    agent any

    stages {
         stage('Compiling Stage') {
            steps {
            	
                echo 'mvn clean compile'
            }
        }
         stage('Testing Stage') {
            steps {

                echo 'mvn test'
            }
        }
         stage('Deployment Stage') {
            steps {
                
                echo 'mvn deploy'
            }
        }
        stage('Release Stage') {
            steps {
                
                echo 'mvn release'
            }
        }
    }
}
